<?xml version="1.0" encoding="UTF-8"?>
<ivy-settings>
  <settings defaultResolver="main" />
  <!--Authentication required for publishing (deployment). 'Artifactory Realm' is the realm used by Artifactory so don't change it.-->
  <credentials host="artifactory.ctlt.ubc.ca" realm="Artifactory Realm" username="admin" passwd="" />
  <resolvers>
    <chain name="main">
      <ibiblio name="ctlt-repo" m2compatible="true" root="http://artifactory.ctlt.ubc.ca/artifactory/repo" />
      <ibiblio name="bb-repo" m2compatible="true" root="https://maven.blackboard.com/content/repositories/releases/" />
      <ibiblio name="public" m2compatible="true"/>
    </chain>
  </resolvers>
</ivy-settings>
